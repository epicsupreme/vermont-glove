    "1659991072ef98a4cd": {
      "type": "history",
      "settings": {
        "title": "1960s",
        "subhead": "",
        "text": "<p>Business booms, selling in 48 states, to electric, cable and telephone companies<\/p>",
        "image": "shopify:\/\/shop_images\/Lineworker_1967.gif",
        "button_link": "link",
        "button_text": "",
        "button_url": "",
        "primary_color": "#452c80",
