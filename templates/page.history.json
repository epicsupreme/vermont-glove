    "1659991297d130ce96": {
      "type": "history",
      "settings": {
        "title": "1980-85",
        "subhead": "",
        "text": "<p>U.S.A manufacturing begins to be widely outsourced overseas<\/p>",
        "image": "shopify:\/\/shop_images\/VTG_Factory_1961.gif",
        "button_link": "link",
        "button_text": "",
        "button_url": "",
        "primary_color": "#452C80",
